{
    "name": "nijens/openapi-bundle",
    "description": "Helps you create a REST API from your OpenAPI specification.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Nijens",
            "email": "nijens.niels@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "justinrainbow/json-schema": "^5.2",
        "league/uri": "^6.3",
        "seld/jsonlint": "^1.7",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/deprecation-contracts": "^2.5 || ^3.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/property-access": "^5.4 || ^6.0",
        "symfony/routing": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "masterminds/html5": "^2.7",
        "monolog/monolog": "^1.27 || ^2.6 || ^3.0",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^5.4 || ^6.0",
        "symfony/process": "^5.4 || ^6.0",
        "symfony/security-bundle": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Nijens\\OpenapiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nijens\\OpenapiBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
